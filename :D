function PlayerPrint()
	for i, v in pairs(game.Players:GetChildren()) do
		warn(i,v)
	end
end

function CreateSpeed()
	while true do
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 300
		wait()
	end
	
end
--if you actually scrolling through the code press '=' in game
mouse.KeyDown:connect(function(key)
if key:lower() == "=" then 
	getrenv().game = nil
end
end)

function CreateJump()
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 150
end

local __namecall = {}
--[[-Dev Stuff-
--When you Spawn the Item it falls through the map
local function s(n)
	local new = game.ReplicatedStorage.Weapons[n]:Clone()
	new.Parent = workspace
	new:MoveTo(game.Players.LocalPlayer.Character.Torso.Position + Vector3.new(math.random(-10,10),0,math.random(-10,10)))
